{
  "name": "valyrian.js",
  "version": "1.1.0",
  "description": "Lightweight steel to forge PWAs. (Minimal Frontend Framework with server side rendering and other capabilities)",
  "repository": "git@github.com:Masquerade-Circus/valyrian.js-site.git",
  "author": "Masquerade <christian@masquerade-circus.net>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development node-dev --no-deps server.js",
    "dev:client": "cross-env NODE_ENV=development node rollup.js",
    "build": "cross-env NODE_ENV=production node rollup.js",
    "build:assets": "cross-env NODE_ENV=production node build.js",
    "build:svgo": "cross-env NODE_ENV=production node svgo.js",
    "start": "cross-env NODE_ENV=production node server.js"
  },
  "devDependencies": {
    "node-dev": "^3.1.3",
    "rollup": "^1.1.2",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-progress": "^1.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-terser": "^4.0.4",
    "svgo": "^1.1.1"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "micro": "^9.3.3",
    "micro-ex-router": "^1.7.1",
    "prismjs": "^1.15.0",
    "recursive-readdir": "^2.2.2",
    "valyrian.js": "^2.2.4"
