{
  "name": "valyrian.js",
  "version": "1.1.0",
  "description": "Lightweight steel to forge PWAs. (Minimal Frontend Framework with server side rendering and other capabilities)",
  "repository": "git@github.com:Masquerade-Circus/valyrian.js-site.git",
  "author": "Masquerade <christian@masquerade-circus.net>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development node-dev --no-deps --require esm server.js",
    "dev:client": "cross-env NODE_ENV=development node rollup.js",
    "dev": "run-all 'yarn dev:client' 'yarn dev:server'",
    "build": "cross-env NODE_ENV=production node rollup.js",
    "build:assets": "cross-env NODE_ENV=production node build.js",
    "build:svgo": "cross-env NODE_ENV=production node svgo.js",
    "start": "cross-env NODE_ENV=production node --require esm server.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "node-dev": "^5.1.0",
    "rollup": "^2.23.0",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "run-all": "^1.0.1",
    "svgo": "^1.3.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "esm": "^3.2.25",
    "micro": "^9.3.4",
    "micro-ex-router": "^1.7.2",
    "prismjs": "^1.20.0",
    "source-map-support": "^0.5.19",
    "valyrian.js": "^4.3.2"
  },
  "now": {
    "alias": "valyrianjs"
  }
}
